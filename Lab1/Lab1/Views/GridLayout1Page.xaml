             x:Class="Lab1.Views.GridLayout1Page"
             xmlns:vm="clr-namespace:Lab1.ViewModels"
             Title="{Binding Title}"
             Visual="Material">

    <ContentPage.BindingContext>
        <vm:GridLayout1ViewModel />
    </ContentPage.BindingContext>

    <ContentPage.Resources>
        <ResourceDictionary>
            <Color x:Key="Accent">#96d1ff</Color>
        </ResourceDictionary>
    </ContentPage.Resources>

</ContentPage>